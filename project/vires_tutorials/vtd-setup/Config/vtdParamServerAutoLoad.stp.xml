<ViSimulation>
<Component name="TaskControl">

<TaskControl xmlns="http://www.vires.com/2015/VtdParamSchema/TaskControl">

  <RDB>
    <sendLightPos value="false"/>
    <sendScoring value="false"/>
    <sendLightSources value="false"/>
    <sendEnvironment value="false"/>
    <sendDriverCtrl value="true"/>
    <sendContactPoints value="true"/>
    <sendRoadState value="true"/>
    <sendEngine value="true"/>
    <sendDrivetrain value="true"/>
    <sendWheels value="true"/>
  </RDB>

  <Video>
    <buffer value="color"/>
  </Video>

  <Sync>
    <source value="RDB"/>
  </Sync>

  <Debug>
    <enable value="false"/>
    <sync value="false"/>
  </Debug>

</TaskControl>
</Component>

</ViSimulation>
