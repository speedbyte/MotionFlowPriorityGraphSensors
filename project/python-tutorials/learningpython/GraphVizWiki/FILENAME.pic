# Written by Sebastian Setzer

.PS
copy "sequence.pic";

# Object definition
object(M,"ucMedia");
object(C,"ucCAN");
step();
active(M);
active(C);

# Message exchange
message(M,C,"WD");
message(C,M,"ACK");
message(C,M,"firstBAPframe")
message(M,C,"ACK");
message(M,C,"answerBAPframe")
message(C,M,"ACK");
